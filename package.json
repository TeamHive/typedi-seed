{
    "name": "typedi-seed",
    "version": "0.0.1",
    "description": "This is the typedi-seed for Team Hive",
    "main": "index.js",
    "scripts": {
        "test": "nyc  -r lcov -e .ts -x \"*.spec.ts\" jasmine-ts \"src/**/*.spec.ts\"",
        "test:debug": "node --inspect-brk=5858 node_modules/.bin/jasmine-ts \"src/**/*.spec.ts\"",
        "prestart": "npm run compile",
        "start": "gulp start",
        "start:debug": "npm run compile && nodemon --inspect-brk=5858 dist/index.js",
        "compile": "npm run clear && tsc",
        "clear": "rm -rf dist"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/TeamHive/typedi-seed.git"
    },
    "keywords": [
        "TypeScript",
        "JavaScript",
        "TypeDI"
    ],
    "contributors": [
        "John Pinkster <jpinkster>",
        "Tyler Vander Maas <tvmaasjazz>"
    ],
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/TeamHive/typedi-seed/issues"
    },
    "homepage": "https://github.com/TeamHive/typedi-seed#readme",
    "dependencies": {
        "@teamhive/typedi-common": "file:../typedi-common/teamhive-typedi-common-1.0.0.tgz",
        "config": "^3.0.1",
        "email-templates": "^5.0.0",
        "log4js": "^4.0.2",
        "nodemailer": "^5.1.1",
        "nodemailer-sendgrid": "^1.0.3",
        "pug": "^2.0.3",
        "raven": "^2.3.0",
        "reflect-metadata": "^0.1.12",
        "typedi": "^0.8.0"
    },
    "devDependencies": {
        "@types/config": "0.0.34",
        "@types/email-templates": "^3.5.0",
        "@types/node": "^11.9.4",
        "@types/nodemailer": "^4.6.6",
        "@types/raven": "^2.1.4",
        "gulp": "^3.9.1",
        "gulp-nodemon": "^2.2.1",
        "gulp-tslint": "^8.1.2",
        "gulp-typescript": "^5.0.0-alpha.3",
        "nyc": "^13.0.1",
        "tslint": "^5.9.1",
        "typescript": "^3.0.3"
    }
}
